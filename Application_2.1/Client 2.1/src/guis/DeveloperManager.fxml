               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </PasswordField>
            <Label text="New Password :">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <PasswordField maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
               <padding>
                  <Insets bottom="8.0" left="10.0" right="10.0" top="8.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </PasswordField>
            <Label text="New Password Again:">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <PasswordField maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
               <padding>
                  <Insets bottom="8.0" left="10.0" right="10.0" top="8.0" />
               </padding>
            </PasswordField>
            <Button mnemonicParsing="false" text="Save">
               <padding>
                  <Insets bottom="6.0" left="30.0" right="30.0" top="6.0" />
