            </PasswordField>
            <Button mnemonicParsing="false" text="Save">
               <padding>
                  <Insets bottom="6.0" left="30.0" right="30.0" top="6.0" />
               </padding>
            </Button>
         </children>
         <padding>
            <Insets bottom="40.0" left="100.0" right="100.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </VBox>
      <VBox style="-fx-background-color: #fff;">
         <children>
            <Label text="Change Password">
               <font>
                  <Font size="13.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Label>
            <Label text="Old Password :" />
            <PasswordField maxWidth="300.0" minWidth="300.0" prefWidth="300.0">
               <padding>
                  <Insets bottom="8.0" left="10.0" right="10.0" top="8.0" />
