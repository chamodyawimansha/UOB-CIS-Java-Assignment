<VBox maxWidth="700.0" minHeight="550.0" minWidth="700.0" prefWidth="700.0" style="-fx-background-color: #eee;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.DeveloperManagerController">
   <padding>
